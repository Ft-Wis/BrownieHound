<Window x:Class="BrownieHound.packet_detail_Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BrownieHound"
        mc:Ignorable="d"
        Title="packet_detail_Window" Height="450" Width="800">
    <Grid>
        <Image Source="logoimage/packet_detail.png" HorizontalAlignment="Left" Height="340" VerticalAlignment="Top"  Margin="-2,-82,0,0"/>
        <TreeView x:Name="detail_tree" ItemsSource="{Binding}" Margin="22,104,22,30" Background="#FFE5E5E5"/>
    </Grid>
</Window>
