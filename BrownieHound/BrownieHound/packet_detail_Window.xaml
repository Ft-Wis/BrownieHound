<Window x:Class="BrownieHound.packet_detail_Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BrownieHound"
        mc:Ignorable="d"
        Title="packet_detail_Window" Height="450" Width="800">
    <Grid>
        <Label x:Name="title" Content="パケット詳細画面" HorizontalAlignment="Left" Height="44" Margin="41,36,0,0" VerticalAlignment="Top" Width="238" FontSize="24"/>
        <TreeView x:Name="detail_tree" ItemsSource="{Binding}" Margin="22,104,22,30" Background="#FFE5E5E5">
        </TreeView>
    </Grid>
</Window>
